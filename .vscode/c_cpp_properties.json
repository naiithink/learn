{
    "env": {
        "envMacClangVersion": "13.1.6",
        "envMacIncludePath": [
            "/usr/local/include",
            "/Library/Developer/CommandLineTools/usr/lib/clang/${env:envMacClangVersion}/include",
            "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include",
            "/Library/Developer/CommandLineTools/usr/include",
            "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/System/Library/Frameworks"
        ]
    },
    "configurations": [
        {
            "name": "macos-clang-arm64",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:envMacIncludePath}"
            ],
            "defines": [],
            "macFrameworkPath": ["/System/Library/Frameworks"],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "macos-clang-arm64"
        },
        {
            "name": "macos-gcc-12-arm64",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:envMacIncludePath}"
            ],
            "defines": [],
            "macFrameworkPath": ["/System/Library/Frameworks"],
            "compilerPath": "/opt/homebrew/bin/gcc-12",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "macos-gcc-arm64"
        },
        {
            "name": "macos-gcc-11-arm64",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:envMacIncludePath}"
            ],
            "defines": [],
            "macFrameworkPath": ["/System/Library/Frameworks"],
            "compilerPath": "/opt/homebrew/bin/gcc-11",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "macos-gcc-arm64"
        },
        {
            "name": "linux-gcc-x64",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "macFrameworkPath": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        }
    ],
    "version": 4
}
